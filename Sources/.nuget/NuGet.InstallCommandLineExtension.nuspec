<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>NuGet.InstallCommandLineExtension</id>
    <version>1.0</version>
    <title>NuGet InstallCommandLineExtension Cmdlet</title>
    <authors>Xavier Decoster</authors>
    <owners>Xavier Decoster</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://github.com/xavierdecoster/NuGet.Analyze/blob/master/COPYRIGHT.txt</licenseUrl>
